    "def isPalindrome(x):\n",
    "    backward = 0\n",
    "    forward = x #unprocessedNumber\n",
    "    while(forward != 0):\n",
    "        backward = (forward % 10) + (backward * 10)\n",
    "        forward = (int) (forward / 10) \n",
    "    return backward == x\n",
    "    "
